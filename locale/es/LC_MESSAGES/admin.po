msgid "Bans a user by name or ucid"
msgstr "Banea a un usuario por nombre o UCID"

msgid "Reason"
msgstr "Razón"

msgid "Days (empty = forever)"
msgstr "Días (vacío = para siempre)"

msgid "Ban Details"
msgstr "Detalles del baneo"

msgid "Member {} is not linked!"
msgstr "El miembro {} no está vinculado!"

msgid "{} is not a valid UCID!"
msgstr "¡{} no es una UCID válida!"

