# /reload
msgid "Reloads a plugin"
msgstr "Carga de nuevo un plugin"

msgid "Plugin {} reloaded."
msgstr "Plugin {} cargado de nuevo."

msgid "Plugin {} could not be reloaded, check the log for details."
msgstr "Plugin {} no pudo ser cargado de nuevo, compruebe el fichero log para más detalles."

msgid "All plugins reloaded."
msgstr "Todos los plugins han sido cargados de nuevo."

msgid "One or more plugins could not be reloaded, check the log for details."
msgstr "Uno o más plugins no han podido ser cargados de nuevo, por favor compruebe el fichero log para más detalles."

# /node add_instance
msgid "Add/create an instance\n"
msgstr "Añade o crea una instancia\n"

msgid "Either select an existing instance or enter the name of a new one"
msgstr "Selecciones el nombre de una instancia existente o una nueva"

msgid "Take this instance configuration as a reference"
msgstr "Utilice la configuración de ésta instancia como referencia"

msgid "Instance \"{}\" created.\nDo you want to configure a server for this instance?"
msgstr "Instancia \"{}\" creada.\n¿Desea configurar el servidor para esta instancia?"

msgid "Server {server} assigned to instance {instance}."
msgstr "Server {server} asignado a la instancia {instance}."

msgid "Instance {} created blank with no server assigned."
msgstr "Instancia {} creada en blanco sin servidor asignado."

msgid "Instance {instance} added to node {node}."
"Please make sure you forward the following ports:"
"```"
"- DCS Port:    {dcs_port} (TCP/UDP)"
"- WebGUI Port: {webgui_port} (TCP)"
"```"
msgstr "Instancia {instance} añadidoa al nodo {node}."
"Por favor, asegúrese de abrir los siguientes puertos:"
"```"
"- Puerto DCS:    {dcs_port} (TCP/UDP)"
"- Puerto WebGUI: {webgui_port} (TCP)"
"```"

msgid "Instance {instance} could not be added to node {node}, see log."
msgstr "Instancia {instance} no pudo ser añadida al nodo {node}, por favor compruebe el log."

