# /node shutdown
msgid "Shuts a specific node down"
msgstr "Apaga un nodo específico"

# /node restart
msgid "Restarts a specific node"
msgstr "Reinicia un nodo específico"

# /node offline
msgid "Shuts down all servers, enables maintenance"
msgstr "Apaga todos los servidores, habilita el modo mantenimiento"

msgid "Node {} is now offline."
msgstr "Nodo {} está ahora fuera de línea."

# /node online
msgid "Clears the maintenance mode for all servers"
msgstr "Desactiva el modo mantenimiento para todos los servidores"

msgid "Node {} is now online."
msgstr "Nodo {} está ahora en línea."

# /node upgrade
msgid "Upgrade DCSServerBot"
msgstr "Actualizar DCSServerBot"

msgid "There is no upgrade available for "
msgstr "No hay actualización disponible para "

msgid "your cluster"
msgstr "tu clúster"

msgid "node {}"
msgstr "nodo {}"

