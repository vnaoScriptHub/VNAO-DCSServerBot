msgid "Do you want to remove the directory {}?"
msgstr "Desea eliminar el directorio {}?"

msgid "Instance {instance} removed from node {node}."
msgstr "Instance {instance} eliminada del nodo {node}."

# /node rename_instance
msgid "Rename an instance\n"
msgstr "Cambie el nombre de una instancia um\n"

msgid "Server {} has to be shut down before renaming the instance!"
msgstr "Servidor {} ha sido apagado antes de renombrar la instancia!"

msgid "Do you really want to rename instance {}?"
msgstr "¿Realmente desea renombrar la instancia {}?"

msgid "Instance {old_name} renamed to {new_name}."
msgstr "Intancia {old_name} renombrada a {new_name}."

