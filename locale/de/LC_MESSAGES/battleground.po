# /battlegound
msgid "DCSBattleground commands"
msgstr "DCSBattleground-Befehle"

# /battlegound recon
msgid "Push MGRS coordinates with screenshots to DCS Battleground"
msgstr "Veröffentliche MRGS-Koordinaten und Screenshots in DCS Battleground"

msgid "The second parameter needs to be a MGRS coordinate (ex: 38TLN0274366889)."
msgstr "Der zweite Parameter muss eine MRGS-Koordinate sein (z. B. 38TLN0274366889)."

msgid "Recon data added - {side} side - {server}"
msgstr "Recon-Daten zugefügt - Server: {server}, Seite: {side}"

msgid "Coalitions have to be enabled and you need to use this command in one of your coalition channels!"
msgstr "Du musst Koalitionen aktivieren und den Befehl in einem Koalitionskanal ausführen!"

# /battlegound reset
msgid "Delete recon data on a specified server"
msgstr "Lösche Recon-Daten"

msgid "Recon data deleted for server {}."
msgstr "Recon-Daten im Server {} gelöscht."