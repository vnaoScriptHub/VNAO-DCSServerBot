msgid "Mod {mod} updated to version {version}."
msgstr "Mod {mod} wurde auf Version {version} aktualisiert."

msgid "Mod {mod} installed with version {version}."
msgstr "Mod {mod}_v{version} wurde installiert."

# /mods uninstall
msgid "Uninstall mods"
msgstr "Deinstalliere Mods"

msgid "Server {} needs to be shut down to uninstall mods."
msgstr "Server {} muss zur Deinstallation von Mods heruntergefahren werden."

