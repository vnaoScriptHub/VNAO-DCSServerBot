msgstr "(De-)installiere Mods in Server {}"

# /mods install
msgid "Install mods"
msgstr "Installiere Mods"

msgid "Server {} needs to be shut down to install mods."
msgstr "Server {} muss zur installation von Mods heruntergefahren werden."

msgid "Mod {} not found!"
msgstr "Mod {} nicht gefunden!"

msgid "Mod {mod}_v{version} is already installed!"
msgstr "Mod {mod}_v{version} ist bereits installiert!"

msgid "Updating mod {mod} from {current_version} to {new_version}, please wait ..."
