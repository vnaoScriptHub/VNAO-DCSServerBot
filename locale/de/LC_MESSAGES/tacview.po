# Plugin
# /tacview
msgid "Commands to manage Tacview"
msgstr "Befehle zur Verwaltung von Tacview"

# /tacview configure
msgid "Configure Tacview"
msgstr "Konfiguriere Tacview"

msgid "Tacview not installed on server {}!"
msgstr "Tacview ist nicht auf Server {} installiert!"

msgid "Tacview configuration changed on server {}."
msgstr "Die Tacview-Konfiguration auf Server {} wurde aktualisiert."

msgid "Server {} needs to be shut down to configure Tacview."
msgstr "Server {} muss zur Konfiguration von Tacview heruntergefahren werden."

# /tacview install
msgid "Install Tacview"
msgstr "Installiere Tacview"

msgid "Tacview already installed on server {}!"
msgstr "Tacview is bereits auf Server {} installiert!"

msgid "Installing Tacview on server {} ..."
msgstr "Installiere Tacview auf Server {} ..."

msgid "Tacview is not configured on node {}!"
msgstr "Tacview ist auf Knoten nicht {} konfiguriert!"

msgid "Tacview installed on server {}."
msgstr "Tacview wurde auf Server {} installiert."

msgid "Tacview could not be installed on server {}!"
msgstr "Tacview konnte auf Server {} nicht installiert werden!"

msgid "Server {} needs to be shut down to install Tacview."
msgstr "Server {} muss zur Installation von Tacview heruntergefahren werden."

# /tacview uninstall
msgid "Uninstall Tacview"
msgstr "De-installiere Tacview"

msgid "Uninstalling Tacview on server {} ..."
msgstr "De-installierer Tacview auf Server {} ..."

msgid "Tacview uninstalled on server {}."
msgstr "Tacview auf Server {} de-installiert."

msgid "Tacview could not be uninstalled on server {}!"
msgstr "Tacview konnte auf Server {} nicht deinstalliert werden!"

msgid "Server {} needs to be shut down to uninstall Tacview."
msgstr "Server {} muss zur De-Installation von Tacview heruntergefahren werden."

