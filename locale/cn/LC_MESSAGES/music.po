# /music
msgid "Commands to manage music in your (DCS) server"
msgstr "用于控制 DCS 服务器上的音乐的命令"

# /music player
msgid "Music Player"
msgstr "音乐播放器"

msgid "You don't have any playlists to play. Please create one with {}."
msgstr "您没有可播放的播放列表.请使用 {} 创建一个."

msgid "Frequency"
msgstr "频道频率"

msgid "Coalition"
msgstr "阵营"

msgid "Red"
msgstr "红方"

msgid "Blue"
msgstr "蓝方"

msgid "Neutral"
msgstr "中立"

msgid "n/a"
msgstr "n/a"

msgid " Now Playing "
msgstr " 现在播放 "

msgid "Title"
msgstr "标题"

msgid "Artist"
msgstr "艺术家"

msgid "Album"
msgstr "专辑"

msgid " Playlist "
msgstr " 播放列表 "

msgid "- empty -"
msgstr "- 空 -"

msgid "Pick a song from the list"
msgstr "从列表中选择一首歌曲"

msgid "Use /music play to access all songs in the list.\n"
msgstr "使用 /music play 访问列表中的所有歌曲.\n"

msgid "Pick a playlist to play"
msgstr "选择要播放的播放列表"

msgid "Stop"
msgstr "停止"

msgid "Play"
msgstr "播放"

msgid "Skip"
msgstr "跳过"

msgid "Repeat"
msgstr "重复"

msgid "Shuffle"
msgstr "随机播放"

msgid "Change Settings for {}"
msgstr "更改 {} 的设置"

msgid "Frequency (xxx.xx)"
msgstr "频道频率 (xxx.xx)"

msgid "Modulation (AM | FM)"
msgstr "调制 (AM | FM)"

msgid "Volume"
msgstr "音量"

msgid "Coalition (1=red | 2=blue)"
msgstr "阵营 (1=红方 | 2=蓝方)"

msgid "Display Name"
msgstr "显示名称"

msgid "Modulation must be one of AM | FM!"
msgstr "调制必须是 AM 或者 FM!"

msgid "Coalition must be 1 or 2!"
msgstr "阵营必须是 1 或者 2!"

# /music play
msgid "Play a song or a playlist\n"
msgstr "播放歌曲或播放列表\n"

msgid "Server {} is not running."
msgstr "服务器 {} 未运行."

msgid "Now playing {} ..."
msgstr "正在播放 {} ..."

msgid "Radio {} started."
msgstr "电台 {} 已开始."

# /music stop
msgid "Stop playing"
msgstr "停止播放"

msgid "Radio {} stopped."
msgstr "电台 {} 已停止."

# /playlist
msgid "Commands to manage music playlists"
msgstr "用于管理播放列表的命令"

# /playlist add
msgid "Add a song to a playlist"
msgstr "将歌曲添加到播放列表"

msgid "{title} has been added to playlist {playlist}."
msgstr "{title} 已添加到播放列表 {playlist}."

# /playlist add_all
msgid "Add all available songs to a playlist"
msgstr "将所有可用歌曲添加到播放列表"

msgid "Do you really want to add ALL songs to the playlist?"
msgstr "您确实要将所有歌曲添加到播放列表中吗?"

# /playlist delete
msgid "Remove a song from a playlist"
msgstr "从播放列表中删除歌曲"

msgid "Playlist {} deleted."
msgstr "播放列表 {} 已删除."

msgid "{title} has been removed from playlist {playlist}."
msgstr "{title} 已从播放列表 {playlist} 中删除"

# upload
msgid "File exists. Do you want to overwrite it?"
msgstr "文件已存在. 是​​否要覆盖它?"

msgid "Song {} uploaded."
msgstr "歌曲 {} 已上传."

msgid "Error {status} while reading file {file}!"
msgstr "读取文件 {file} 时发生错误 {status} !"
